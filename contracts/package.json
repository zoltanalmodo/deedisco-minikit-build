{
  "name": "deedisco-minikit-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Deedisco Minikit NFT collection",
  "scripts": {
    "compile": "hardhat compile",
    "deploy:local": "hardhat run scripts/deploy-local.js --network localhost",
    "deploy:base-sepolia": "hardhat run scripts/deploy-simple.js --network baseSepolia",
    "deploy:testnet": "hardhat run scripts/deploy.js --network baseSepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network base",
    "verify:testnet": "hardhat verify --network baseSepolia",
    "verify:mainnet": "hardhat verify --network base"
  },
  "devDependencies": {
    "hardhat": "^2.26.3"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.3",
    "ethers": "^5.8.0"
  }
}
